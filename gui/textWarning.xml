	<Frame name="DTM_TextWarning" setAllPoints="true" parent="UIParent" frameStrata="HIGH">
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="DTM_TextWarning_FontString" inherits="NumberFontNormalHuge" justifyH="MIDDLE" justifyV="MIDDLE"/>
			</Layer>
		</Layers>

		<Scripts>
			<OnLoad>
				DTM_TextWarning_OnLoad(self);
			</OnLoad>
			<OnUpdate>
				DTM_TextWarning_OnUpdate(self, elapsed);
			</OnUpdate>
		</Scripts>
	</Frame>
</UI>